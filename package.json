{
  "name": "article-connectivity-visualization",
  "private": true,
  "scripts": {
    "bundle": "browserify article/main.js --debug > article/bundle.js",
    "bundle-watch": "watchify article/main.js --debug -o article/bundle.js",
    "bundle-production": "browserify article/main.js -t babelify > article/bundle.js",
    "start": "http-server article",
    "postinstall" : "npm run bundle-production"
  },
  "dependencies": {
    "d3-array": "^1.2.1",
    "d3-axis": "^1.0.8",
    "d3-collection": "^1.0.4",
    "d3-fetch": "^1.1.0",
    "d3-format": "^1.3.2",
    "d3-interpolate": "^1.1.6",
    "d3-path": "^1.0.5",
    "d3-scale": "^1.0.6",
    "d3-scale-chromatic": "^1.2.0",
    "d3-selection": "^1.1.0",
    "d3-shape": "^1.2.0",
    "d3-time": "^1.0.8",
    "d3-time-format": "^2.1.1",
    "deeplearn": "0.5.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-plugin-transform-async-generator-functions": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-minify": "^0.3.0",
    "babelify": "^8.0.0",
    "browserify": "^16.2.3",
    "http-server": "^0.11.1",
    "watchify": "^3.11.0"
  }
}
